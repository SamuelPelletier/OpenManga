<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {# On boucle sur les URLs #}
    {% for url in urls %}
        <sitemap>
            <loc>{{ app.request.getSchemeAndHttpHost() ~ url }}</loc>
        </sitemap>
    {% endfor %}
</sitemapindex>
